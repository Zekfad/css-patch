{
	"type": "module",
	"main": "./dist/cjs/bundle.cjs",
	"exports": {
		".": {
			"import": "./dist/esm/bundle.mjs",
			"require": "./dist/cjs/bundle.cjs",
			"default": "./dist/esm/bundle.cjs"
		}
	},
	"module": "./dist/esm/bundle.js",
	"name": "css-patch",
	"version": "1.1.0",
	"description": "CSS patch generator.",
	"keywords": [
		"css",
		"diff",
		"patch",
		"stylesheets",
		"stylesheet",
		"transformer"
	],
	"homepage": "https://github.com/Zekfad/css-patch",
	"bugs": {
		"url": "https://github.com/Zekfad/css-patch/issues",
		"email": "zekfad@znnme.eu.org"
	},
	"license": "ISC",
	"author": "Zekfad <zekfad@znnme.eu.org> (https://zekfad.znnme.eu.org)",
	"repository": "github:Zekfad/css-patch",
	"types": "types/index.d.ts",
	"scripts": {
		"lint": "eslint . --ext .js,.cjs,.mjs",
		"dev": "rollup -c --watch --environment mode:dev",
		"build": "run-p build:**",
		"build:src": "rollup -c",
		"build:docs": "jsdoc -c .jsdoc.json --verbose",
		"build:def": "tsc",
		"test": "nyc --reporter=lcov mocha",
		"coverage": "codecov"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.12.11",
		"@rollup/plugin-babel": "^5.2.1",
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-json": "^4.0.2",
		"@rollup/plugin-node-resolve": "^11.1.0",
		"@types/diff": "^5.0.0",
		"@types/stylis": "^4.0.0",
		"@zekfad/eslint-config": "^1.0.0",
		"babel-eslint": "^10.1.0",
		"codecov": "^3.7.2",
		"eslint": "^7.18.0",
		"eslint-plugin-import": "^2.22.1",
		"jsdoc": "^3.6.6",
		"magic-string": "^0.25.7",
		"mocha": "^8.1.3",
		"npm-run-all": "^4.1.5",
		"nyc": "^15.1.0",
		"rollup": "^2.38.0",
		"rollup-plugin-terser": "^7.0.2",
		"tui-jsdoc-template": "^1.2.2",
		"typescript": "^4.0.3"
	},
	"engines": {
		"node": ">=10"
	},
	"dependencies": {
		"diff": "^5.0.0",
		"stylis": "^4.0.6"
	}
}
